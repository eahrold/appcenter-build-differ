{
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "indent": [ "error", 4 ],
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "import/no-unresolved": 0
  },
  "settings": {
  }
}
